

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <link href="~/Content/Style.css" rel="stylesheet" />
</head>
<body>
    <div class="row">
        <div class="col-sm-4">
            <img id="img" src="~/content/Images/geek.png" alt="Geek" />
        </div>
        <div id="heading" class="col-lg-offset-5">
            <h1>National Park Geek</h1>
        </div>
    <nav class="navbar navbar-inverse">
        <ul class="nav navbar-nav">
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            <li>@Html.ActionLink("Survey", "Survey", "Home")</li>
        </ul>
        <p class="navbar-text">Welcome to National Park Geek!</p>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />

    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/salvattore.min.js"></script>

</body>
</html>
